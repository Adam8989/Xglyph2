<?xml version="1.0" encoding="utf-8"?>

<layer-list
	xmlns:android="http://schemas.android.com/apk/res/android">

	<item>

		<shape
			android:shape="rectangle">

			<solid
				android:color="@color/rectangle_cyan_fill" />

			<stroke
				android:color="@color/rectangle_cyan"
				android:width="@dimen/rectangle_width" />
		</shape>

	</item>

	<item
		android:top="32dp">

		<shape
			android:shape="line">

			<stroke
				android:color="@color/rectangle_cyan"
				android:width="@dimen/rectangle_width" />

			<corners></corners>
		</shape>
	</item>
</layer-list>
